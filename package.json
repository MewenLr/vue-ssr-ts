{
  "name": "vue-ssr",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "analyze": "webpack --config ./webpack/webpack.prod.js --mode=production --client --analyze",
    "build": "npm run build:client && npm run build:server",
    "build:client": "webpack --config ./webpack/webpack.prod.js --mode=production --client --hide-modules",
    "build:server": "webpack --config ./webpack/webpack.prod.js --mode=production --server --hide-modules",
    "dev": "webpack-dev-server --config ./webpack/webpack.dev.js --mode=development",
    "lint": "npm run lint:script && npm run lint:style",
    "lint:info": "eslint --ext .js,.ts,.vue ./ && stylelint ./**/*.vue ./**/*.sass ./**/*.scss",
    "lint:script": "eslint --ext .js,.ts,.vue ./ --fix",
    "lint:style": "stylelint ./**/*.vue ./**/*.sass ./**/*.scss --fix",
    "postbuild": "rm ./public/index.html",
    "prod": "npm run build && npm run server",
    "server": "node ./server/index.js",
    "test": "jest"
  },
  "dependencies": {
